{
    "name": "ponponpaon",
    "version": "1.0.0",
    "description": "vode player",
    "main": "dist/main.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "rimraf dist && run-p dev:webpack dev:electron",
        "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress --watch",
        "dev:electron": "wait-on dist/index.html && electronmon .",
        "build": "cross-env NODE_ENV=\"production\" webpack --progress",
        "format": "prettier --write 'src/**/*.{js,ts,json}'",
        "lint": "npm-run-all eslint check-types",
        "lint:fix": "npm-run-all eslint:fix check-types format",
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make"
    },
    "keywords": [],
    "author": "RT-EGG",
    "license": "ISC",
    "electronmon": {
        "patterns": [
            "dist/**/*"
        ]
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.8.3",
        "@electron-forge/maker-deb": "^7.8.3",
        "@electron-forge/maker-rpm": "^7.8.3",
        "@electron-forge/maker-squirrel": "^7.8.3",
        "@electron-forge/maker-zip": "^7.8.3",
        "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
        "@electron-forge/plugin-fuses": "^7.8.3",
        "@electron/fuses": "^1.8.0",
        "@eslint/js": "^9.35.0",
        "@reduxjs/toolkit": "^2.9.0",
        "@types/node": "^24.3.1",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "@types/uuid": "^10.0.0",
        "cross-env": "^10.0.0",
        "css-loader": "^7.1.2",
        "electron": "^38.0.0",
        "electronmon": "^2.0.3",
        "eslint": "^9.35.0",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-react": "^7.37.5",
        "ffmpeg-static": "^5.2.0",
        "ffprobe-static": "^3.1.0",
        "framer-motion": "^4.1.17",
        "globals": "^16.3.0",
        "html-webpack-plugin": "^5.6.4",
        "image-size": "^2.0.2",
        "mini-css-extract-plugin": "^2.9.4",
        "npm-run-all": "^4.1.5",
        "react-redux": "^9.2.0",
        "rimraf": "^6.0.1",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.42.0",
        "uuid": "^13.0.0",
        "wait-on": "^8.0.4",
        "webpack": "^5.101.3",
        "webpack-cli": "^6.0.1"
    },
    "dependencies": {
        "@fontsource/roboto": "^5.2.6",
        "@tailwindcss/cli": "^4.1.13",
        "daisyui": "^5.1.8",
        "electron-squirrel-startup": "^1.0.1",
        "next-themes": "^0.4.6",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-icons": "^5.5.0",
        "styled-components": "^6.1.19",
        "tailwindcss": "^4.1.13",
        "webpack-dev-server": "^5.2.2"
    }
